    <!-- http://purl.obolibrary.org/obo/BFO_0000006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000006">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO spatial region</rdfs:comment>
        <rdfs:label>spatial region</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000015 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000015">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO process</rdfs:comment>
        <rdfs:label>process</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000016 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000016">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO disposition</rdfs:comment>
        <rdfs:label>disposition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000019 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000019">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO quality</rdfs:comment>
        <rdfs:label>quality</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000023 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000023">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO role</rdfs:comment>
        <rdfs:label>role</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000024 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000024">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:comment>BFO fiat object part</rdfs:comment>
        <rdfs:label>fiat object part</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000030 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000030">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:comment>BFO  object</rdfs:comment>
        <rdfs:label>object</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000040 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000040">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO material Entity</rdfs:comment>
        <rdfs:label>material entity</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000140 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000140">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>BFO continuant fiat boundary</rdfs:comment>
        <rdfs:label>continuant fiat boundary</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000145 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/BFO_0000145">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label>relational quality</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/BoundaryBoundaryRelation -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/BoundaryBoundaryRelation">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000145"/>
        <rdfs:comment>Boundary/Boundary Relation is a BFO: Relational Quality that depends on two Geological Boundaries. It describes the space and/or time relations between these boundaries.     </rdfs:comment>
        <rdfs:label>boundary boundary relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/BoundaryBoundarySpaceRelation -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/BoundaryBoundarySpaceRelation">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/BoundaryBoundaryRelation"/>
        <rdfs:comment>Boundary/Boundary Space Relation is a Boundary/Boundary Relation which describes the space relation between two Stratigraphic Boundaries.    </rdfs:comment>
        <rdfs:label>boundary boundary space relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/BoundaryBoundaryTimeRelation -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/BoundaryBoundaryTimeRelation">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/BoundaryBoundaryRelation"/>
        <rdfs:comment>Boundary/Boundary Time Relation is a Boundary/Boundary Relation which describes the time relation (older / younger / co-eval with) between two Stratigraphic Boundaries.  </rdfs:comment>
        <rdfs:label>boundary boundary time relation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/CurrentTimeGeologicalObject -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/CurrentTimeGeologicalObject">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000024"/>
        <rdfs:comment> BFO: Fiat Object Part attached to the Geological Object Earth Crust or to, some portion of it. It occupies a Region of Interest and characterizes at any Geological Time (current time), the global geology in the Region of Interest, i.e. the geological entities present and their relations. An instance of Current Time Global Geology is the result of the succession of processes that operated in the Region of Interest before the instant at which the instance is defined.    </rdfs:comment>
        <rdfs:label>current time geological object</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/DepostionalStratigraphicUnit -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/DepostionalStratigraphicUnit">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/GeologicalUnit"/>
        <rdfs:comment>Depositional Stratigraphic Unit is a Geological Unit. generated by a depositional process during a definite Geological Time Interval. </rdfs:comment>
        <rdfs:label>depostional stratigraphic unit</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/ErosionSurface -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/ErosionSurface">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/StratigraphicSurface"/>
        <rdfs:comment>Erosion Surface is a Stratigraphic Surface which signals the final stage of an erosion process that destroyed some earth material In an older Geological Occurrence.   An Erosion Surface is covered by one or several younger Stratigraphic Units.  </rdfs:comment>
        <rdfs:label>erosion surface</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/FluidCompartement -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/FluidCompartement">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000024"/>
        <rdfs:comment>Fluid Compartment is a BFO: fiat Object part corresponding to a Reservoir portion containing one specific Earth Fluid (Water Oil, Gas or a mixture of those).</rdfs:comment>
        <rdfs:label>fluid compartement</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/FluidInterface -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/FluidInterface">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/GeologicalBoundary"/>
        <rdfs:comment>Fluid Interface is a BFO: 2D Continuant Fiat Boundary attached to a reservoir. It corresponds to a horizontal surface separating two Fluid Compartments. </rdfs:comment>
        <rdfs:label>fluid interface</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/FluidRetention -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/FluidRetention">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <rdfs:comment>A disposition that inheres in a geological unit or some of its parts, realized when the unit or some of its parts have sufficient porosity and capillary forces to store Earth Fluids within its pore spaces</rdfs:comment>
        <rdfs:label>fluid retention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/FluidTransmission -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/FluidTransmission">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
        <rdfs:comment>A disposition that inheres in a geological unit or some of its parts, realized when the unit or some of its parts possess sufficient permeability and connected pore networks to facilitate the movement of Earth Fluids through the rock matrix or fractures.</rdfs:comment>
        <rdfs:label>fluid transmission</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/Fold -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/Fold">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000004"/>
        <rdfs:comment>Fold is GeoCore Geological Structure which is the result of the bending or curving of a stack of originally planar surfaces like sedimentary surfaces  during permanent deformation.</rdfs:comment>
        <rdfs:label>fold</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalBlock -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalBlock">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000001"/>
        <rdfs:comment> Geological Block is a GeoCore: Geological Object which results from the partition of a geological unit by a later geological process which may be an erosion or a faulting  or thrusting process.</rdfs:comment>
        <rdfs:label>geological block</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalBlockBoundary -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalBlockBoundary">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/GeologicalBoundary"/>
        <rdfs:comment>Geological Block Boundary is a BFO: 2D Continuant Fiat Boundary which corresponds to the outer surface of a Geological Block. It consists in several portions, which are each coplanar with a specific Geological Boundary   </rdfs:comment>
        <rdfs:label>geological block boundary</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalBoundary -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalBoundary">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000140"/>
        <rdfs:comment> GeoCore updated definition: Geological Boundary is a BFO: 2D Continuant Fiat Boundary attached and intrinsically related to a Geological Object. A Geological Boundary materializes the Time Instant at which some Geological Process began or ceased operating   </rdfs:comment>
        <rdfs:label>geological boundary</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalBoundaryAge -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalBoundaryAge">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000003"/>
        <rdfs:comment>Geological Boundary Age is a Quality of- inhering in Geological Boundary.  It is related to the Time instant that this Geological Boundary materializes. </rdfs:comment>
        <rdfs:label>geological boundary age</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalObjectAge -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalObjectAge">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000003"/>
        <rdfs:comment>GeoCore Definition :Geological Object Age is a Quality inhering in Geological Object. It is related to the Geological Time Interval during which the process that generated this Geological Object operated. </rdfs:comment>
        <rdfs:label>geological object age</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalUnit -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalUnit">
        <rdfs:subClassOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000001"/>
        <rdfs:comment>Geological Unit is a GeoCore: Geological Object defined at mapping scale, which is the result of a rock generative process: sediment deposition, magmatic intrusion or of metamorphism.</rdfs:comment>
        <rdfs:label>geological unit</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/GeologicalUnitPart -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/GeologicalUnitPart">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000024"/>
        <rdfs:comment>Geological Unit Part is GeoCore Geological Object corresponding to a portion of Geological Unit having some particular Quality (for instance a sandstone lens inside a shale layer).  </rdfs:comment>
        <rdfs:label>geological unit part</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/InterestRegion -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/InterestRegion">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000006"/>
        <rdfs:comment>Region of Interest is a BFO: Spatial Region. It designates the region which is object of the modeling  </rdfs:comment>
        <rdfs:label>interest region</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/MagmaticIntrusiveGeobody -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/MagmaticIntrusiveGeobody">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/GeologicalUnit"/>
        <rdfs:comment>Magmatic Intrusive Geobody is a Geological Unit which results of a magma intrusion process. A granite batholith, magmatic sills or dykes are examples of Magmatic Intrusive Geobody.</rdfs:comment>
        <rdfs:label>magmatic intrusive geobody</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/MagmaticIntrusiveOuterSurface -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/MagmaticIntrusiveOuterSurface">
        <rdfs:subClassOf rdf:resource="http://www.brgm.geomod_ont/GeologicalBoundary"/>
        <rdfs:comment>Magmatic Intrusive Outer Surface is a Geological Boundary which corresponds to the outer envelope of a Magmatic Intrusive Unit. A  Magmatic Intrusive Outer Surface is younger than the Geological Occurrence that it intrudes. .   </rdfs:comment>
        <rdfs:label>magmatic intrusive outer surface </rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/PetrophysicalProperty -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/PetrophysicalProperty">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:comment>Petrophysical Property is a BFO Specifically Dependent Continuant inhering in GeoCore: Rock. It refers to some rock physical property like Color or Porosity (BFO: Quality) or Permeability (BFO: Disposition).</rdfs:comment>
        <rdfs:label>petrophysical property</rdfs:label>
    </owl:Class>
    


    <!-- http://www.brgm.geomod_ont/Polarity -->

    <owl:Class rdf:about="http://www.brgm.geomod_ont/Polarity">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
