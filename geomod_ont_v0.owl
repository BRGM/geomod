<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.brgm.geomod_ont#"
     xml:base="http://www.brgm.geomod_ont"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.brgm.geomod_ont"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.obolibrary.org/obo/BFO_0000058 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000058">
        <rdfs:comment>BFO concretized by</rdfs:comment>
        <rdfs:label>concretized by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000196 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000196">
        <owl:inverseOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000197"/>
        <rdfs:comment>BFO bearer of relation (has quality)</rdfs:comment>
        <rdfs:label>bearer of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/BFO_0000197 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/BFO_0000197">
        <rdfs:comment>BFO inherers in</rdfs:comment>
        <rdfs:label>inheres in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/boundaryOf -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/boundaryOf">
        <owl:inverseOf rdf:resource="http://www.brgm.geomod_ont/hasBoundary"/>
        <rdfs:comment>inverse relation of has boundary</rdfs:comment>
        <rdfs:label>boundary of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/coveredBy -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/coveredBy">
        <rdfs:subPropertyOf rdf:resource="http://www.brgm.geomod_ont/hasBoundary"/>
        <rdfs:comment>Covered by Is a relation related-to between a BFO: Object and a Polarized Boundary.  It specifies that the Object is in contact with the Old-Face of the Polarized Boundary.  </rdfs:comment>
        <rdfs:label>covered By</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/hasBoundary -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/hasBoundary">
        <rdfs:comment>a relation that relates a geological object to its geological boundary</rdfs:comment>
        <rdfs:label>has boundary</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/hasChronoBottom -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/hasChronoBottom">
        <owl:inverseOf rdf:resource="http://www.brgm.geomod_ont/chronoBottomOf%20"/>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/hasChronoTop -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/hasChronoTop">
        <owl:inverseOf rdf:resource="http://www.brgm.geomod_ont/chronoTopOf%20"/>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/hasDisposition -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/hasDisposition">
        <rdfs:comment>a relation between a continuant and the dispostion it disposes, e,g,, a geological unit has diposition to retain fluids</rdfs:comment>
        <rdfs:label>has disposition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/hasPart -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/hasPart">
        <rdfs:comment>a parthood relation to  designates mereological sums by rlating entities to their parts (modified after BFO)</rdfs:comment>
        <rdfs:label>has part</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/laysOver -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/laysOver">
        <rdfs:subPropertyOf rdf:resource="http://www.brgm.geomod_ont/hasBoundary"/>
        <rdfs:comment>Lays over Is a relation related-to between a BFO: Object and a Polarized Boundary.  It specifies that the Object is in contact with the Young-Face of the Polarized Boundary.   </rdfs:comment>
        <rdfs:label>lays over</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/outerBoundaryOf -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/outerBoundaryOf">
        <rdfs:subPropertyOf rdf:resource="http://www.brgm.geomod_ont/boundaryOf"/>
        <rdfs:comment>a specific relation to relate a Magmatic Intrusive Geobody or Geological block to its boundary</rdfs:comment>
        <rdfs:label>outer boundary of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/plays -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/plays">
        <rdfs:comment>relation between a continuant and the role it plays, e,g,, a part of a geological unit or a block can plays the role of a reservoir when it has the disposition for fluid retention, fluid transmission and it does contains the fluid  </rdfs:comment>
        <rdfs:label>plays</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/spatiallyRelatedTo -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/spatiallyRelatedTo">
        <rdfs:comment>an relation to indicates that a geological object, geological structure, or a geological boundary is related to one of these entities through a spatial relation (Allen&apos;s relations)</rdfs:comment>
        <rdfs:label>spatially related to</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/chronoBottomOf%20 -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/chronoBottomOf%20">
        <rdfs:subPropertyOf rdf:resource="http://www.brgm.geomod_ont/boundaryOf"/>
        <rdfs:comment>ChronoBottom of is a relation related-to between a Horizon and a Stratigraphic Unit.  It specifies that the Horizon is the manifestations of the beginning of the deposition process that created the unit. </rdfs:comment>
        <rdfs:label>chrono Bottom Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.brgm.geomod_ont/chronoTopOf%20 -->

    <owl:ObjectProperty rdf:about="http://www.brgm.geomod_ont/chronoTopOf%20">
        <rdfs:subPropertyOf rdf:resource="http://www.brgm.geomod_ont/boundaryOf"/>
        <rdfs:comment>ChronoTop of Is a relation related-to between a Horizon and a Stratigraphic Unit.  It specifies that the Horizon is the manifestations of the endof the deposition process that created the unit. </rdfs:comment>
        <rdfs:label>chrono Top Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/bdi/ontologies/BFO_0000101 -->

    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/bdi/ontologies/BFO_0000101">
        <rdfs:comment>GeoCore carrier of</rdfs:comment>
        <rdfs:label>carrier of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000013 -->

    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000013">
        <rdfs:comment>GeoCore generated by</rdfs:comment>
        <rdfs:label>generated by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000015 -->

    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000015">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
        <owl:inverseOf rdf:resource="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000016"/>
        <rdfs:comment>GeoCore definition : The relation between a Geological Object and its Geological Age.</rdfs:comment>
        <rdfs:label>has age</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000016 -->

    <owl:ObjectProperty rdf:about="https://www.inf.ufrgs.br/bdi/ontologies/GEOCORE_0000016">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000197"/>
        <rdfs:comment>GeoCore definition : The relation between a Geological Age and a Geological Object.</rdfs:comment>
        <rdfs:label>age of</rdfs:label>
    </owl:ObjectProperty>
    


